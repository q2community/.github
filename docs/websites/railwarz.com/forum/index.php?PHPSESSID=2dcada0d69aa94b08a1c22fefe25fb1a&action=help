                function shrinkHeaderLeftbar(mode)
                {
                        document.cookie = "upshrink=" + (mode ? 1 : 0);
                        document.getElementById("upshrinkLeftbar").src = smf_images_url + (mode ? "/upshrink2.gif" : "/upshrink.gif");
